@model IEnumerable<MailTicketAzureMvc.Models.ApplicationUser>

<h2>@ViewBag.Title.</h2>

<h3>@ViewBag.Message</h3>
<p>Numero oggetti rilevati in RecuperaEventiMail: @ViewBag.Service</p>
@ViewBag.filtro
<div class="row">
    @foreach (var item in ViewBag.Eventi)
    {
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="~/Content/Images/demo.jpg" />
                    <div class="caption">
                        <h5>idSpettacolo @item.idSpettMail</h5>
                        <h5>idSpettMail @item.idSpettMail</h5>
                        <h3>@item.Spettacolo</h3>
                        <h4>@item.idMan</h4>
                        <p>@item.Location</p>
                        <p>@item.Dataevento</p>
                    </div>
                </div>
            </div>
    }
</div>
<div class="btn-group">
    <input id="Button1" type="submit" value="button" />
    <button type="button" class="btn btn-default">Left</button>
    <button type="button" class="btn btn-default">Middle</button>
    <button type="button" class="btn btn-default">Right</button>
</div>
